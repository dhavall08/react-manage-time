{
  "hosting": {
    "site": "do-timely",
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "rewrites": [
    {
      "source": "/robots.txt",
      "destination": "/robots.txt"
    },
    {
      "source": "/sitemap",
      "destination": "/sitemap.xml"
    }
  ]
}
